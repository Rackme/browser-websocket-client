{
  "name": "browser-websocket-client",
  "version": "1.0.1",
  "description": "Connect to a WebSocket server to send and receive raw JSON messages.",
  "license": "Apache-2.0",
  "author": "Ken Siprell",
  "homepage": "https://github.com/kensiprell/browser-websocket-client/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/kensiprell/browser-websocket-client.git"
  },
  "scripts": {
    "clean": "rimraf build dist",
    "clean:chrome": "rimraf build/dev/chrome",
    "clean:firefox": "rimraf build/dev/firefox",
    "clean:chrome:prod": "rimraf build/prod/chrome dist/chrome.zip",
    "clean:firefox:prod": "rimraf build/prod/firefox dist/firefox.zip",
    "build:chrome": "npm run clean:chrome && webpack --watch --config webpack.config.js --env=dev:chrome --progress --colors",
    "build:firefox": "npm run clean:firefox && webpack --watch --config webpack.config.js --env=dev:firefox --progress --colors",
    "build:dev": "npm run build:chrome && npm run build:firefox",
    "build:chrome:prod": "npm run clean:chrome:prod && webpack --config webpack.config.js --env=prod:chrome --progress --colors",
    "build:firefox:prod": "npm run clean:firefox:prod && webpack --config webpack.config.js --env=prod:firefox --progress --colors",
    "build:prod": "npm run build:chrome:prod && npm run build:firefox:prod",
    "lint": "eslint -c .eslintrc.json src/**/*.js",
    "open": "npm run open:unit && npm run open:e2e",
    "open:e2e": "opener ./protractor_html/htmlReport_BrowserWebSocketClient.html",
    "open:unit": "opener ./karma_html/Karma_Report/index.html",
    "postinstall": "webdriver-manager update",
    "test": "npm run lint && npm run test:unit && npm run test:e2e && npm run open",
    "test:e2e": "npm run build:chrome:prod && protractor protractor.config.js",
    "test:unit": "karma start",
    "zip:chrome": "npm run build:chrome:prod && mkdirp ./dist && zip -jr dist/chrome.zip build/prod/chrome/*",
    "zip:firefox": "npm run build:firefox:prod && mkdirp ./dist && zip -jr dist/firefox.zip build/prod/firefox/*",
    "zip:all": "npm run zip:chrome && npm run zip:firefox"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.2",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "popper.js": "1.12.6"
  },
  "devDependencies": {
    "@types/chrome": "0.0.52",
    "@types/jasmine": "2.6.3",
    "@types/selenium-webdriver": "3.0.8",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "0.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.28.7",
    "eslint": "4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-preact": "1.1.6",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "file-loader": "1.1.5",
    "font-awesome-webpack": "0.0.5-beta.2",
    "generate-json-webpack-plugin": "0.2.2",
    "html-loader": "0.5.1",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.6",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mkdirp": "0.5.1",
    "node-sass": "4.6.0",
    "opener": "1.4.3",
    "postcss-loader": "2.0.8",
    "precss": "2.0.0",
    "protractor": "5.2.0",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-merge": "4.1.1"
  }
}
